{
    "tabWidth": 2,
    "useTabs": false,
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "vueIndentScriptAndStyle": false,
    "embeddedLanguageFormatting": "auto",
    "overrides": [
      {
        "files": ".prettierrc",
        "options": {
          "parser": "json"
        }
      }
    ],
    "parser": "babel",
    "plugins": [
      "prettier-plugin-svelte"
    ]
  }
  